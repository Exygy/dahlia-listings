dl.application-form_nav.sub-nav ng-show="hasNav()"
  dd ng-repeat="section in sections" ng-class="{active: isActiveSection(section)}"
    a ui-sref="dahlia.short-form-application.{{ section.pages[0] }}" ng-class="{disabled: !(isActiveSection(section) || isPreviousSection(section))}"
      | {{section.name}}
